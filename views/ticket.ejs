          <div id="mainListDiv" class="main_list">
            <ul class="navlinks">
              <li><a href="/">Accueil</a></li>
              <li><a href="programmation">Programmation</a></li>
              <li class="nav-item dropdown">
                <a
                  class="nav-link dropdown-toggle"
                  href="#"
                  id="navbarDropdown"
                  role="button"
                  data-toggle="dropdown"
                  aria-haspopup="true"
                  aria-expanded="false"
                >
                  Le Festival
                </a>
                <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                  <a class="dropdown-item" href="trajet"
                    >Se rendre à APOKATOA</a
                  >
                  <a class="dropdown-item" href="hebergement">Hébergement</a>
                  <a class="dropdown-item" href="#">Manger et boire</a>
                  <a class="dropdown-item" href="#">Prévention</a>
                </div>
              </li>
              <li><a href="about-us">À propos</a></li>
              <li><a href="contact">Contact</a></li>
            </ul>
          </div>
          <span class="navTrigger">
            <i></i>
            <i></i>
            <i></i>
          </span>
        </div>
      </nav>
      <!-- header-bar -->
    </header>

    <div class="page-header">
      <div class="container">
        <div class="row">
          <div class="col-12">
            <div class="entry-header">
              <h2 class="entry-title">Tickets</h2>

              <ul class="breadcrumbs flex align-items-center">
                <li><a href="/">Accueil</a></li>
                <li>Acheter un ticket</li>
              </ul>
              <!-- .breadcrumbs -->
            </div>
            <!-- entry-header -->
          </div>
          <!-- col-12 -->
        </div>
        <!-- row -->
      </div>
      <!-- container -->
    </div>
    <!-- page-header -->

    <div class="main-content">
      <div class="container">
        <div class="entry-header">
          <div class="entry-title">
            <p>Lorum epsum</p>
            <h2>Tickets de Bonne Heure</h2>
          </div>
          <!-- entry-title -->
        </div>
        <!-- entry-header -->

        <div class="row">
          <div class="col-12">
            <div class="tabs">
              <ul class="tabs-nav flex">
                <li
                  class="tab-nav flex justify-content-center align-items-center active"
                  data-target="#tab_details"
                >
                  Details
                </li>
                <li></li>
                <li></li>
                <li></li>
              </ul>
              <!-- tabs-nav -->

              <div class="tabs-container">
                <div id="tab_details" class="tab-content">
                  <div class="flex flex-wrap justify-content-between">
                    <div class="single-event-details">
                      <div class="single-event-details-row">
                        <label>Début :</label>
                        <p>09 Septembre 2022</p>
                      </div>

                      <div class="single-event-details-row">
                        <label>Fin :</label>
                        <p>10 Septembre 2022</p>
                      </div>

                      <div class="single-event-details-row">
                        <label>Prix :</label>
                        <p class="sold-out">0€</p>
                      </div>

                      <div class="single-event-details-row">
                        <label>Tags:</label>
                        <p>
                          <a href="#">festivals</a>, <a href="#">musique</a>,
                          <a href="#">concert</a>
                        </p>
                      </div>
                    </div>

                    <div class="single-event-map">
                      <iframe
                        id="gmap_canvas"
                        src="https://maps.google.com/maps?q=university of san francisco&t=&z=15&ie=UTF8&iwloc=&output=embed"
                        frameborder="0"
                        scrolling="no"
                        marginheight="0"
                        marginwidth="0"
                      ></iframe>
                    </div>
                  </div>
                </div>
                <!-- .tab-content -->
                <!-- .tab-content -->
              </div>
              <!-- .tabs-container -->
            </div>
            <!-- .tabs -->
          </div>
          <!-- .col-12 -->
        </div>
        <!-- .row -->

        <div class="row">
          <div class="col-12">
            <div class="event-tickets">
              <div
                class="ticket-row flex flex-column flex-lg-row justify-content-lg-between align-items-lg-center"
              >
                <div
                  class="ticket-type flex flex-column flex-lg-row align-items-lg-center"
                >
                  <h3 class="entry-title">Silver Ticket</h3>

                  <span class="mt-2 mt-lg-0">Entrée basique</span>

                  <div class="ticket-price mt-3 mt-lg-0">0€</div>
                  <!-- ticket-price -->
                </div>
                <!-- ticket-type -->

                <div class="flex align-items-center">
                  <div
                    class="number-of-tickets flex justify-content-between align-items-center"
                  >
                    <span class="decrease-ticket">-</span>
                    <input
                      type="number"
                      name=""
                      value="1"
                      class="ticket-count"
                    />
                    <span class="increase-ticket">+</span>
                  </div>
                  <!-- number-of-tickets -->

                  <div class="clear-ticket-count">Supprimer</div>
                </div>
                <!-- flex -->

                <input
                  type="submit"
                  name=""
                  value="Ajouter"
                  class="btn mt-2 mb-2 mt-lg-0 mb-lg-0"
                /><!-- btn -->
              </div>
              <!-- ticket-row -->
              <!-- ticket-row -->
            </div>
            <!-- event-tickets -->
          </div>
          <!-- col-12 -->
        </div>
        <!-- row -->
      </div>
      <!-- container -->
    </div>
