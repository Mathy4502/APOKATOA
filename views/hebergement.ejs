                    <svg viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
                      <path fill="#3997DE" d="M39.6,-44.5C52.8,-36.1,66,-25.1,70.4,-10.9C74.7,3.3,70.2,20.7,61,33.8C51.7,46.9,37.7,55.9,21.8,63.2C5.9,70.4,-11.9,76,-28.8,72.6C-45.7,69.3,-61.7,57.1,-66.8,41.7C-71.8,26.4,-65.9,7.9,-62.8,-11.3C-59.6,-30.4,-59.2,-50.2,-49.2,-59.2C-39.2,-68.1,-19.6,-66.2,-3.2,-62.3C13.2,-58.5,26.4,-52.9,39.6,-44.5Z" transform="translate(100 100)" />
